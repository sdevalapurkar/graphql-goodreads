{
  "name": "graphql-goodreads",
  "version": "1.0.0",
  "description": "GraphQL service providing data about your favourite books",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run rest\" \"npm run apollo\"",
    "apollo": "node apollo",
    "graphql": "node server",
    "rest": "json-server --watch data.json --port 3005"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdevalapurkar/graphql-goodreads.git"
  },
  "author": "sdevalapurkar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sdevalapurkar/graphql-goodreads/issues"
  },
  "homepage": "https://github.com/sdevalapurkar/graphql-goodreads#readme",
  "dependencies": {
    "apollo-server": "2.14.2",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.0.0",
    "node-fetch": "2.6.1",
    "concurrently": "5.2.0",
    "json-server": "0.16.1"
  }
}
